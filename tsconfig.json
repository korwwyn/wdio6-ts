{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "*": [ "./*" ],
      "src/*": ["./src/*"]
    },
    "outDir": "src/",
    "typeRoots": ["./types"],
    "types": ["node", "@wdio/sync", "@wdio/mocha-framework"]
  },
  "include": [
    "./test/**/*.ts",
    "./pageObjects/*.ts",
    "./helpers/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}